type UserId = principal;
type User = 
 record {
   createdAt: Time;
   id: UserId;
   username: text;
   wallet: text;
 };
type Time = int;
service : {
  getAllUsers: () -> (vec User) query;
  getUser: (userId: UserId) -> (opt User) query;
  registerUser: (username: text, wallet: text) -> (bool);
}
